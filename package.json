{
  "name": "phase3-brand-content-factory",
  "version": "3.0.0",
  "description": "Brand Content Factory - Phase 3B (Brand Analysis) + Phase 3C (Video Director Mode)",
  "type": "module",
  "scripts": {
    "dev": "tsx scripts/sentinel.ts && next dev",
    "build": "tsx scripts/sentinel.ts && next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "report:arch": "tsx scripts/generate-arch-report.ts",
    "sentinel": "tsx scripts/sentinel.ts",
    "ask:cto": "tsx scripts/consult-cto.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.90.11",
    "@trpc/client": "^11.7.2",
    "@trpc/react-query": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "next": "^14.2.33",
    "openai": "^4.24.0",
    "postgres": "^3.4.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "superjson": "^2.2.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.33",
    "glob": "^13.0.0",
    "husky": "^9.1.7",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
